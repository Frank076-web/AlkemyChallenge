owMore(!showMore);\r\n    };\r\n\r\n    //Funcion para eliminar el heroe del equipo\r\n    const deleteHero = () => {\r\n        if (\r\n            window.confirm(\r\n                'Are you sure you want to delete this hero?'\r\n            )\r\n        ) {\r\n            let staff = JSON.parse(localStorage.getItem('staff'));\r\n            let newStaff = staff.filter((hero) => {\r\n                return hero.id !== props.heroe.id;\r\n            });\r\n            localStorage.removeItem('staff');\r\n            localStorage.setItem('staff', JSON.stringify(newStaff));\r\n            window.location.reload();\r\n        }\r\n    };\r\n\r\n    // console.log(props.heroe);\r\n\r\n    let image = props.heroe.image.url,\r\n        name = props.heroe.name;\r\n    return (\r\n        <div\r\n            className=\"container\"\r\n            style={{\r\n                marginTop: '2rem'\r\n            }}\r\n        >\r\n            <div className=\"card\">\r\n                <div className=\"card-body\">\r\n                    <img\r\n                        className=\"card-img-top\"\r\n                        src={image}\r\n                        alt={name}\r\n                    />\r\n                    <h5 className=\"card-title text-center\">{name}</h5>\r\n                    {/* Visualizacion de los stats */}\r\n                    <div\r\n                        className=\"card-header text-center\"\r\n                        style={{\r\n                            fontSize: '1.5rem',\r\n                            fontWeight: 'bold'\r\n                        }}\r\n                    >\r\n                        Status\r\n                    </div>\r\n                    <ul className=\"list-group list-group-flush\">\r\n                        {\r\n                            <HeroCardStats\r\n                                stats={props.heroe.powerstats}\r\n                            />\r\n                        }\r\n                    </ul>\r\n\r\n    